package com.eif.data;

import javax.persistence.Entity;
@Entity
@Table(name="Product_Table")

public class Product {
@Id
int id;
String name;
int Quantity;

public product() {
	super();
}
	public Product(int id, String name, int quantity) {
	super();
	this.id = id;
	this.name = name;
	Quantity = quantity;
}



}
